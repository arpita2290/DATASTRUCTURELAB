// 1
#include <stdio.h>
int main() {
    printf("Hello, World!");
    return 0;
}

// 2
#include <stdio.h>
int main() {
    int a, b;
    scanf("%d %d", &a, &b);
    printf("%d", a + b);
    return 0;
}

// 3
#include <stdio.h>
int main() {
    float r;
    scanf("%f", &r);
    printf("%f", 3.14 * r * r);
    return 0;
}

// 4
#include <stdio.h>
int main() {
    int a, b, temp;
    scanf("%d %d", &a, &b);
    temp = a;
    a = b;
    b = temp;
    printf("%d %d", a, b);
    return 0;
}

// 5
#include <stdio.h>
int main() {
    int a, b;
    scanf("%d %d", &a, &b);
    if (a > b)
        printf("%d", a);
    else
        printf("%d", b);
    return 0;
}

// 6
#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    if (n % 2 == 0)
        printf("Even");
    else
        printf("Odd");
    return 0;
}

// 7
#include <stdio.h>
int main() {
    int year;
    scanf("%d", &year);
    if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0)
        printf("Leap Year");
    else
        printf("Not Leap Year");
    return 0;
}

// 8
#include <stdio.h>
int main() {
    int n, i, fact = 1;
    scanf("%d", &n);
    for (i = 1; i <= n; i++)
        fact *= i;
    printf("%d", fact);
    return 0;
}

// 9
#include <stdio.h>
int main() {
    int n, i;
    scanf("%d", &n);
    for (i = 1; i <= 10; i++)
        printf("%d x %d = %d\n", n, i, n * i);
    return 0;
}

// 10
#include <stdio.h>
int main() {
    int n, rev = 0;
    scanf("%d", &n);
    while (n != 0) {
        rev = rev * 10 + n % 10;
        n /= 10;
    }
    printf("%d", rev);
    return 0;
}

// 11
#include <stdio.h>
int main() {
    int n, i, flag = 1;
    scanf("%d", &n);
    if (n < 2) flag = 0;
    for (i = 2; i <= n / 2; i++)
        if (n % i == 0) {
            flag = 0;
            break;
        }
    if (flag) printf("Prime");
    else printf("Not Prime");
    return 0;
}

// 12
#include <stdio.h>
int main() {
    int n, sum = 0;
    scanf("%d", &n);
    while (n != 0) {
        sum += n % 10;
        n /= 10;
    }
    printf("%d", sum);
    return 0;
}

// 13
#include <stdio.h>
int main() {
    int n, a = 0, b = 1, c, i;
    scanf("%d", &n);
    for (i = 0; i < n; i++) {
        printf("%d ", a);
        c = a + b;
        a = b;
        b = c;
    }
    return 0;
}

// 14
#include <stdio.h>
int main() {
    char ch;
    scanf("%c", &ch);
    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' ||
        ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U')
        printf("Vowel");
    else
        printf("Consonant");
    return 0;
}

// 15
#include <stdio.h>
int main() {
    float c;
    scanf("%f", &c);
    printf("%f", (c * 9 / 5) + 32);
    return 0;
}
 
ðŸ”¹ Intermediate Level (16â€“30)


// 16
#include <stdio.h>
int main() {
    int a, b, i, gcd;
    scanf("%d %d", &a, &b);
    for (i = 1; i <= a && i <= b; i++)
        if (a % i == 0 && b % i == 0)
            gcd = i;
    printf("GCD=%d\n", gcd);
    printf("LCM=%d", (a * b) / gcd);
    return 0;
}

// 17
#include <stdio.h>
int main() {
    int n, count = 0;
    scanf("%d", &n);
    while (n != 0) {
        n /= 10;
        count++;
    }
    printf("%d", count);
    return 0;
}

// 18
#include <stdio.h>
int main() {
    int i, j, flag;
    for (i = 2; i <= 100; i++) {
        flag = 1;
        for (j = 2; j <= i / 2; j++)
            if (i % j == 0) {
                flag = 0;
                break;
            }
        if (flag) printf("%d ", i);
    }
    return 0;
}

// 19
#include <stdio.h>
int main() {
    int n, i, max;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    max = a[0];
    for (i = 1; i < n; i++)
        if (a[i] > max)
            max = a[i];
    printf("%d", max);
    return 0;
}

// 20
#include <stdio.h>
int main() {
    int n, i, j, temp;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    for (i = 0; i < n - 1; i++)
        for (j = i + 1; j < n; j++)
            if (a[i] > a[j]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
    for (i = 0; i < n; i++) printf("%d ", a[i]);
    return 0;
}

// 21
#include <stdio.h>
int main() {
    int r, c, i, j;
    scanf("%d %d", &r, &c);
    int a[r][c], b[r][c], sum[r][c];
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            scanf("%d", &a[i][j]);
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            scanf("%d", &b[i][j]);
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            sum[i][j] = a[i][j] + b[i][j];
    for (i = 0; i < r; i++) {
        for (j = 0; j < c; j++)
            printf("%d ", sum[i][j]);
        printf("\n");
    }
    return 0;
}

// 22
#include <stdio.h>
int main() {
    int r1, c1, r2, c2, i, j, k;
    scanf("%d %d", &r1, &c1);
    int a[r1][c1];
    for (i = 0; i < r1; i++)
        for (j = 0; j < c1; j++)
            scanf("%d", &a[i][j]);
    scanf("%d %d", &r2, &c2);
    int b[r2][c2], res[r1][c2];
    for (i = 0; i < r2; i++)
        for (j = 0; j < c2; j++)
            scanf("%d", &b[i][j]);
    if (c1 != r2) return 0;
    for (i = 0; i < r1; i++)
        for (j = 0; j < c2; j++) {
            res[i][j] = 0;
            for (k = 0; k < c1; k++)
                res[i][j] += a[i][k] * b[k][j];
        }
    for (i = 0; i < r1; i++) {
        for (j = 0; j < c2; j++)
            printf("%d ", res[i][j]);
        printf("\n");
    }
    return 0;
}

// 23
#include <stdio.h>
int main() {
    int r, c, i, j;
    scanf("%d %d", &r, &c);
    int a[r][c], t[c][r];
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            scanf("%d", &a[i][j]);
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            t[j][i] = a[i][j];
    for (i = 0; i < c; i++) {
        for (j = 0; j < r; j++)
            printf("%d ", t[i][j]);
        printf("\n");
    }
    return 0;
}

// 24
#include <stdio.h>
int main() {
    int n, i, pos, val;
    scanf("%d", &n);
    int a[n+1];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    scanf("%d %d", &pos, &val);
    for (i = n; i >= pos; i--)
        a[i] = a[i - 1];
    a[pos - 1] = val;
    for (i = 0; i <= n; i++) printf("%d ", a[i]);
    return 0;
}

// 25
#include <stdio.h>
int main() {
    int n, i, pos;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    scanf("%d", &pos);
    for (i = pos - 1; i < n - 1; i++)
        a[i] = a[i + 1];
    for (i = 0; i < n - 1; i++) printf("%d ", a[i]);
    return 0;
}

// 26
#include <stdio.h>
int main() {
    int n, i, key;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    scanf("%d", &key);
    for (i = 0; i < n; i++)
        if (a[i] == key) {
            printf("Found at %d", i + 1);
            return 0;
        }
    printf("Not Found");
    return 0;
}

// 27
#include <stdio.h>
int main() {
    char str1[100], str2[100];
    scanf("%s", str1);
    int i = 0;
    while (str1[i] != '\0') {
        str2[i] = str1[i];
        i++;
    }
    str2[i] = '\0';
    printf("%s", str2);
    return 0;
}

// 28
#include <stdio.h>
int main() {
    char str[100];
    int i = 0;
    scanf("%s", str);
    while (str[i] != '\0') i++;
    printf("%d", i);
    return 0;
}

// 29
#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    int i, len, flag = 1;
    scanf("%s", str);
    len = strlen(str);
    for (i = 0; i < len / 2; i++)
        if (str[i] != str[len - i - 1]) {
            flag = 0;
            break;
        }
    if (flag) printf("Palindrome");
    else printf("Not Palindrome");
    return 0;
}

// 30
#include <stdio.h>
int main() {
    char str[100];
    int i, v = 0, c = 0, d = 0, s = 0;
    fgets(str, sizeof(str), stdin);
    for (i = 0; str[i] != '\0'; i++) {
        if ((str[i] >= 'a' && str[i] <= 'z') || (str[i] >= 'A' && str[i] <= 'Z'))
            if (str[i] == 'a'|| str[i] == 'e'|| str[i] == 'i'|| str[i] == 'o'|| str[i] == 'u'||
                str[i] == 'A'|| str[i] == 'E'|| str[i] == 'I'|| str[i] == 'O'|| str[i] == 'U') v++;
            else c++;
        else if (str[i] >= '0' && str[i] <= '9') d++;
        else if (str[i] == ' ') s++;
    }
    printf("%d %d %d %d", v, c, d, s);
    return 0;
}


// 31
#include <stdio.h>
int main() {
    char op;
    float a, b;
    scanf(" %c", &op);
    scanf("%f %f", &a, &b);
    switch(op) {
        case '+': printf("%f", a + b); break;
        case '-': printf("%f", a - b); break;
        case '*': printf("%f", a * b); break;
        case '/': if (b != 0) printf("%f", a / b); else printf("Error"); break;
        default: printf("Invalid");
    }
    return 0;
}

// 32
#include <stdio.h>
int fact(int n) {
    if (n == 0) return 1;
    return n * fact(n - 1);
}
int main() {
    int n;
    scanf("%d", &n);
    printf("%d", fact(n));
    return 0;
}

// 33
#include <stdio.h>
int fib(int n) {
    if (n == 0) return 0;
    if (n == 1) return 1;
    return fib(n - 1) + fib(n - 2);
}
int main() {
    int n;
    scanf("%d", &n);
    printf("%d", fib(n));
    return 0;
}

// 34
#include <stdio.h>
int main() {
    int a[5] = {1, 2, 3, 4, 5}, *p;
    p = a;
    for (int i = 0; i < 5; i++)
        printf("%d ", *(p + i));
    return 0;
}

// 35
#include <stdio.h>
void swap(int *a, int *b) {
    int temp = *a;
    *a = *b;
    *b = temp;
}
int main() {
    int x, y;
    scanf("%d %d", &x, &y);
    swap(&x, &y);
    printf("%d %d", x, y);
    return 0;
}

// 36
#include <stdio.h>
void bubble(int a[], int n) {
    int i, j, t;
    for (i = 0; i < n - 1; i++)
        for (j = 0; j < n - i - 1; j++)
            if (a[j] > a[j + 1]) {
                t = a[j];
                a[j] = a[j + 1];
                a[j + 1] = t;
            }
}
int main() {
    int n, i;
    scanf("%d", &n);
    int a[n];
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    bubble(a, n);
    for (i = 0; i < n; i++) printf("%d ", a[i]);
    return 0;
}

// 37
#include <stdio.h>
struct Student {
    char name[50];
    int age;
    float marks;
};
int main() {
    struct Student s;
    scanf("%s %d %f", s.name, &s.age, &s.marks);
    printf("%s %d %.2f", s.name, s.age, s.marks);
    return 0;
}

// 38
#include <stdio.h>
int main() {
    FILE *f;
    char data[100];
    f = fopen("data.txt", "w");
    scanf(" %[^\n]", data);
    fprintf(f, "%s", data);
    fclose(f);
    f = fopen("data.txt", "r");
    fgets(data, 100, f);
    printf("%s", data);
    fclose(f);
    return 0;
}

// 39
#include <stdio.h>
int main() {
    FILE *f1, *f2, *f3;
    char ch;
    f1 = fopen("file1.txt", "r");
    f2 = fopen("file2.txt", "r");
    f3 = fopen("merged.txt", "w");
    while ((ch = fgetc(f1)) != EOF) fputc(ch, f3);
    while ((ch = fgetc(f2)) != EOF) fputc(ch, f3);
    fclose(f1);
    fclose(f2);
    fclose(f3);
    return 0;
}

// 40
#include <stdio.h>
#include <stdlib.h>
int main() {
    int n, i;
    scanf("%d", &n);
    int *a = (int *)malloc(n * sizeof(int));
    for (i = 0; i < n; i++) scanf("%d", &a[i]);
    for (i = 0; i < n; i++) printf("%d ", a[i]);
    free(a);
    return 0;
}




